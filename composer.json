{
  "name": "phplint/phplint",
  "description": "phplint can speed upi your linting my running several processes asynchronously",
  "type": "library",
  "keywords": [
    "phplint",
    "lint",
    "syntax",
    "check"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Martins Sipenko",
      "email": "martins.sipenko@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/console": "2.5.*",
    "symfony/finder": "2.5.*",
    "symfony/process": "2.5.*"
  },
  "autoload": {
    "psr-4": {
      "Phplint\\": "Phplint/"
    }
  },
  "bin": [
    "bin/phplint"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
